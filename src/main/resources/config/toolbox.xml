<xml xmlns="https://developers.google.com/blockly/xml" id="toolbox" style="display: none">
  <category name="Basic">
    <block type="output"></block>
  </category>
  <category name="Aggregate">
    <block type="nbr"></block>
    <block type="branch"></block>
    <block type="mux"></block>
    <block type="rep">
      <field name="PREVIOUS" id="yANW!y:.,l;R6;+zv.#?">x</field>
    </block>
  </category>
  <category name="Utilities">
    <block type="mid"></block>
    <block type="distance_to"></block>
    <block type="channel"></block>
    <block type="distance_between"></block>
  </category>
  <category name="Fold">
    <block type="foldhood"></block>
    <block type="foldhoodplus"></block>
  </category>
  <category name="Sensors">
    <block type="sense">
      <value name="TYPE">
        <block type="class_boolean">
          <field name="NAME">Boolean</field>
        </block>
      </value>
      <value name="SENSOR_NAME">
        <block type="string_value">
          <field name="VALUE">sensorX</field>
        </block>
      </value>
    </block>
  </category>
  <category name="Actuators">
    <block type="led_all_to">
      <value name="COLOR">
        <block type="color_value">
          <field name="VALUE">#ffff00</field>
        </block>
      </value>
    </block>
  </category>
  <category name="Operations">
    <block type="equals"></block>
    <block type="boolean_operation">
      <field name="OPERATOR">&amp;&amp;</field>
    </block>
    <block type="number_compare">
      <field name="OPERATOR">GREATER</field>
    </block>
    <block type="number_operation">
      <field name="OPERATOR">ADDITION</field>
    </block>
  </category>
  <category name="Values">
    <block type="string_value">
      <field name="VALUE"></field>
    </block>
    <block type="integer_value">
      <field name="VALUE">1</field>
    </block>
    <block type="double_value">
      <field name="VALUE">NaN</field>
    </block>
    <block type="boolean_value">
      <field name="VALUE">true</field>
    </block>
    <block type="color_value">
      <field name="VALUE">#ffff00</field>
    </block>
    <block type="tuple_value"></block>
    <block type="random_value"></block>
    <block type="random_value_between"></block>
  </category>
  <category name="Type">
    <block type="class_integer">
      <field name="NAME">Integer</field>
    </block>
    <block type="class_double">
      <field name="NAME">Double</field>
    </block>
    <block type="class_boolean">
      <field name="NAME">Boolean</field>
    </block>
    <block type="class_string">
      <field name="NAME">String</field>
    </block>
    <block type="class_other">
      <field name="NAME"></field>
    </block>
  </category>
  <category name="Function">
    <block type="function_no_return">
      <field name="FUNCTION_NAME">functionName</field>
    </block>
  </category>
</xml>